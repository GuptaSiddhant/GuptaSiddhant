{
  "name": "website",
  "version": "0.0.0",
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "prebuild": "tailwindcss --minify -o ./app/styles/tailwind.css",
    "preprebuild": "yarn clear",
    "dev": "run-p -ncl dev:node dev:css dev:remix",
    "dev:node": "cross-env NODE_ENV=development nodemon ./server/index.js --watch ./server/index.js",
    "dev:css": "tailwindcss -w -o ./app/styles/tailwind.css",
    "dev:remix": "remix watch",
    "start": "cross-env NODE_ENV=production node ./server/index.js",
    "clear": "rm -rf api .cache build public/build"
  },
  "dependencies": {
    "@remix-run/express": "^1.5.0",
    "@remix-run/node": "^1.5.0",
    "@remix-run/react": "^1.5.0",
    "@remix-run/server-runtime": "^1.5.0",
    "clsx": "^1.1.1",
    "gs-constants": "*",
    "gs-helpers": "*",
    "gs-hooks": "*",
    "gs-ui": "*",
    "lru-cache": "^7.10.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "remixicon-react": "^1.0.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.5.0",
    "@remix-run/eslint-config": "^1.5.0",
    "@remix-run/serve": "^1.5.0",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/typography": "^0.5.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/tailwindcss": "^2.2.4",
    "cross-env": "^7.0.3",
    "eslint": "^8.15.0",
    "firebase-admin": "^10.2.0",
    "firebase-functions": "^3.21.0",
    "gs-config": "*",
    "gs-types": "*",
    "nodemon": "^2.0.16",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.0.0"
  },
  "engines": {
    "node": "16"
  },
  "main": "server.js"
}
