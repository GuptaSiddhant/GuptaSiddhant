{
  "name": "website",
  "version": "0.0.0",
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:css build:remix",
    "build:css": "tailwindcss --minify -o ./app/styles/tailwind.css",
    "build:remix": "remix build",
    "dev": "run-p dev:css dev:remix",
    "dev:css": "tailwindcss -w -o ./app/styles/tailwind.css",
    "dev:remix": "remix dev",
    "clear": "rm -rf api .cache build public/build"
  },
  "dependencies": {
    "@remix-run/node": "^1.5.0",
    "@remix-run/react": "^1.5.0",
    "@remix-run/serve": "^1.5.0",
    "clsx": "^1.1.1",
    "gs-constants": "*",
    "gs-helpers": "*",
    "gs-hooks": "*",
    "gs-ui": "*",
    "lru-cache": "^7.10.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "remixicon-react": "^1.0.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.5.0",
    "@remix-run/eslint-config": "^1.5.0",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/typography": "^0.5.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/tailwindcss": "^2.2.4",
    "cross-env": "^7.0.3",
    "gs-config": "*",
    "gs-types": "*",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.0.0"
  },
  "engines": {
    "node": ">=14"
  }
}
